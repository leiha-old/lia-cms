{
  "name": "lia/cms",
  "description": "Lia Framework : CMS",
  "homepage": "https://github.com/leiha/lia-cms/",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Leiha Sellier",
      "email": "leiha.sellier@gmail.com"
    }
