    },
    {
	"type": "vcs",
        "url": "https://github.com/leiha/lia-listing.git",
    },
    {
	"type": "vcs",
        "url": "https://github.com/leiha/lia-crud.git",
    }
  ],
  "require": {
        "lia/kernel": "dev-master@dev",
        "lia/theme": "dev-master@dev",
        "lia/crud": "dev-master@dev",
        "lia/listing": "dev-master@dev",
        "friendsofsymfony/user-bundle": "~2.0@dev"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.x-dev"
    }
  }
